<tr id="<%= user.id %>">
  <td><strong><%= user.name %></strong></td>
  <td><strong><%= user.last_name %></strong></td>
  <td><%= user.email %></td>
  <td>
    <%= link_to "", edit_user_path(user.id), class: "fa fa-pencil-square-o"%>
    <%= link_to "", {action: 'destroy', id: user.id},
          data: {confirm: "Seguro que desea eliminar al usuario?"}, method: :delete,
          class: "fa fa-times destroy-student-icon"%>
  </td>
</tr>
